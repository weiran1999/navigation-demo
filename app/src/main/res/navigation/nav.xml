<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:startDestination="@id/index_screen">

    <fragment
        android:id="@+id/index_screen"
        android:name="com.weiran.navigation.ui.fragment.simple.Index">
        <action
            android:id="@+id/action_title_screen_to_register"
            app:destination="@id/simple_register" />
    </fragment>

      <fragment
          android:id="@+id/simple_register"
          android:name="com.weiran.navigation.ui.fragment.simple.Register">
          <action
              android:id="@+id/action_register_to_match"
              app:destination="@+id/match"/>
      </fragment>

    <fragment
        android:id="@+id/match"
        android:name="com.weiran.navigation.ui.fragment.simple.Match">
    </fragment>


</navigation>